{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": 0,
    "lines-between-class-members": "off",
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off"
  }
}
